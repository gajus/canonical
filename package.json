{
  "name": "canonical",
  "description": "Canonical code style linter and formatter for JavaScript, SCSS and CSS.",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/canonical"
  },
  "keywords": [
    "eslint",
    "scss-lint",
    "scss",
    "css",
    "lint"
  ],
  "bin": "./dist/bin/index.js",
  "version": "1.0.1",
  "author": {
    "name": "Gajus Kuizinas",
    "email": "gk@anuary.com",
    "url": "http://gajus.com"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "readme": "node ./node_modules/.bin/gitdown ./.README/README.md --output-file ./README.md",
    "build": "node ./node_modules/.bin/babel ./src --out-dir ./dist"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "eslint": "^1.4.1",
    "eslint-config-canonical": "^1.0.2",
    "eslint-config-canonical-react": "^1.1.1",
    "eslint-plugin-react": "^3.3.2",
    "globby": "^3.0.1",
    "lodash": "^3.10.1",
    "table": "^3.0.1",
    "yargs": "^3.24.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "gitdown": "^2.4.0"
  }
}
